public class Color {
   public final float red, green, blue;
  
  
   public Color(int red, int green, int blue) {
     this.red = red;
     this.green = green;
     this.blue = blue;
   }
   
   public Color(float red, float green, float blue) {
     this.red = red;
     this.green = green;
     this.blue = blue;
   }
}
